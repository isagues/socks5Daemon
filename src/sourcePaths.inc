ARGS_HANDLER=argsHandler

BUFFER=buffer

NET_UTILS=netutils

PARSERS=parsers/*

SELECTOR=selector

SOCKS5=socks5
STATE_MACHINE_BUILDER=$(SOCKS5)/stateMachineBuilder

STATE_MACHINE=stateMachine

STATISTICS=statistics

STATES_PATH=states/*
ERROR_STATES_PATH=states/errorStates/*
CLOSE=states/close/*

UTILITIES=utilities utilities/*


USER_HANDLER=userHandler

SOURCE_PATHS = $(ARGS_HANDLER) $(BUFFER) $(NET_UTILS) $(PARSERS) $(SELECTOR) $(SOCKS5) $(STATE_MACHINE_BUILDER)
SOURCE_PATHS += $(STATISTICS) $(STATE_MACHINE) $(STATES_PATH) $(ERROR_STATES_PATH) $(CLOSE) $(UTILITIES) $(USER_HANDLER)
